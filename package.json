{
  "name": "ippo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.9.0",
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.22",
    "classnames": "^2.2.6",
    "dayjs": "^1.7.8",
    "firebase": "^5.6.0",
    "graphql": "^14.0.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-dom": "^16.6.3",
    "react-headroom": "^2.2.4",
    "react-infinite-scroller": "^1.2.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-swipeable-views": "^0.13.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "styled-components": "^4.1.2"
  },
  "scripts": {
    "start": "NODE_PATH=. REACT_APP_LOCAL=true react-scripts start",
    "start:aws": "NODE_PATH=. react-scripts start",
    "build": "NODE_PATH=. react-scripts build",
    "test": "NODE_PATH=. react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "NODE_PATH=. start-storybook -p 9009 -s public",
    "build-storybook": "NODE_PATH=. build-storybook -s public",
    "deploy-storybook": "NODE_PATH=. build-storybook -s public && cd storybook-static && git init && git add -A && git commit -m 'deploy storybook' && git push -f git@github.com:ozaki25/ippo.git master:gh-pages",
    "deploy": "yarn build && firebase deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-info": "^4.0.9",
    "@storybook/addon-knobs": "^4.0.9",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-viewport": "^4.0.9",
    "@storybook/addons": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "jest-styled-components": "^6.3.1",
    "prop-types": "^15.6.2",
    "react-mock-router": "^1.0.15",
    "storybook-react-router": "^1.0.2"
  }
}
